<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.custom">

    <!-- 震动 -->
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CustomPropertyActivity" />
        <activity android:name=".MeasureViewActivity" />
        <activity android:name=".DrawRoundActivity" />
        <activity android:name=".RunnableActivity" />
        <activity android:name=".ProgressBarActivity" />
        <activity android:name=".TextProgressActivity" />
        <activity android:name=".ProgressAnimationActivity" />
        <activity android:name=".NotifySimpleActivity" />
        <activity android:name=".NotifyCounterActivity" />
        <activity android:name=".NotifyLargeActivity" />
        <activity android:name=".NotifySpecialActivity" />
        <activity android:name=".NotifyCustomActivity" />
        <activity android:name=".ServiceNormalActivity" />
        <activity android:name=".ServiceBindActivity" />
        <activity android:name=".NotifyServiceActivity" />
        <activity android:name=".VibratorActivity" />
        <activity
            android:name=".FreshPurchaseActivity"
            android:theme="@style/AppCompatTheme" />
        <activity
            android:name=".FreshDetailActivity"
            android:theme="@style/AppCompatTheme" />

        <service
            android:name=".service.NormalService"
            android:enabled="true" />
        <service
            android:name=".service.BindService"
            android:enabled="true" />
        <service
            android:name=".service.MusicService"
            android:enabled="true" />
        <service
            android:name=".service.GroupService"
            android:enabled="true" />

    </application>

</manifest>